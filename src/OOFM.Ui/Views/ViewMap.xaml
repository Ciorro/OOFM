<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:pm="clr-namespace:OOFM.Ui.ViewModels.Pages"
    xmlns:pw="clr-namespace:OOFM.Ui.Views.Pages"
    xmlns:vm="clr-namespace:OOFM.Ui.ViewModels"
    xmlns:vw="clr-namespace:OOFM.Ui.Views"
    xmlns:vmi="clr-namespace:OOFM.Ui.ViewModels.Items"
    xmlns:vwi="clr-namespace:OOFM.Ui.Views.Items">
    <DataTemplate DataType="{x:Type pm:HomePageViewModel}">
        <pw:HomePageView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type pm:StationsPageViewModel}">
        <pw:StationsPageView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type pm:MyMusicPageViewModel}">
        <pw:MyMusicPageView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type pm:HistoryPageViewModel}">
        <pw:HistoryPageView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type pm:BlacklistPageViewModel}">
        <pw:BlacklistPageView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type pm:SettingsPageViewModel}">
        <pw:SettingsPageView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vmi:StationItemViewModel}">
        <vwi:StationItemView />
    </DataTemplate>
</ResourceDictionary>