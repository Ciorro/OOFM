<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:vm="clr-namespace:OOFM.Ui.ViewModels"
    xmlns:vw="clr-namespace:OOFM.Ui.Views">
    <DataTemplate DataType="{x:Type vm:HomePageViewModel}">
        <vw:HomePageView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:StationsListViewModel}">
        <vw:StationsListView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:MyMusicPageViewModel}">
        <vw:MyMusicPageView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:HistoryPageViewModel}">
        <vw:HistoryPageView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:BlacklistPageViewModel}">
        <vw:BlacklistPageView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:SettingsPageViewModel}">
        <vw:SettingsPageView />
    </DataTemplate>
</ResourceDictionary>