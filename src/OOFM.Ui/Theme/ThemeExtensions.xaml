<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:ui="http://schemas.modernwpf.com/2019">
    <ResourceDictionary.MergedDictionaries>
        <ui:ThemeResources>
            <ui:ThemeResources.ThemeDictionaries>
                <ResourceDictionary x:Key="Dark">
                    <SolidColorBrush x:Key="LayerFillColorDefault" Color="#4C3A3A3A" />
                    <SolidColorBrush x:Key="LayerFillColorAlt" Color="#0DFFFFFF" />
                    <ui:StaticResource x:Key="LayerFillColorDefaultBrush" ResourceKey="LayerFillColorDefault" />
                    <ui:StaticResource x:Key="LayerFillColorAltBrush" ResourceKey="LayerFillColorAlt" />
                </ResourceDictionary>
                <ResourceDictionary x:Key="Light">
                    <SolidColorBrush x:Key="LayerFillColorDefault" Color="#80FFFFFF" />
                    <SolidColorBrush x:Key="LayerFillColorAlt" Color="#0A000000" />
                    <ui:StaticResource x:Key="LayerFillColorDefaultBrush" ResourceKey="LayerFillColorDefault" />
                    <ui:StaticResource x:Key="LayerFillColorAltBrush" ResourceKey="LayerFillColorAlt" />
                </ResourceDictionary>
            </ui:ThemeResources.ThemeDictionaries>
        </ui:ThemeResources>
        <ui:XamlControlsResources />
    </ResourceDictionary.MergedDictionaries>
</ResourceDictionary>